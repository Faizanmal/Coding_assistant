  "bin": {
    "myext": "./dist/cli/index.js"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "build:cli": "tsc -p tsconfig.cli.json",
    "watch": "tsc --watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "cli": "npm run build:cli && node dist/cli/index.js"
  },
