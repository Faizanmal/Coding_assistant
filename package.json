{
  "name": "coding",
  "displayName": "Copilot",
  "description": "",
  "version": "0.0.1",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "coding.inFileChat",
        "key": "ctrl+j",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "coding.inFileChat",
          "group": "navigation"
        },
        {
          "command": "coding.fixSelectedError",
          "when": "editorHasSelection",
          "group": "navigation"
        },
        {
          "command": "coding.generateTests",
          "group": "testing"
        },
        {
          "command": "coding.refactorSelection",
          "when": "editorHasSelection",
          "group": "refactoring"
        },
        {
          "command": "coding.generateDocumentation",
          "when": "editorHasSelection",
          "group": "documentation"
        },
        {
          "command": "coding.analyzePerformance",
          "group": "analysis"
        },
        {
          "command": "coding.explainCode",
          "when": "editorHasSelection",
          "group": "assistant"
        },
        {
          "command": "coding.optimizeCode",
          "when": "editorHasSelection",
          "group": "assistant"
        },
        {
          "command": "coding.addLogging",
          "when": "editorHasSelection",
          "group": "quickfix"
        },
        {
          "command": "coding.addErrorHandling",
          "when": "editorHasSelection",
          "group": "quickfix"
        },
        {
          "command": "coding.analyzeError",
          "group": "debug"
        },
        {
          "command": "coding.reviewCurrentFile",
          "group": "review"
        },
        {
          "command": "coding.quickScan",
          "when": "editorHasSelection",
          "group": "review"
        }
      ]
    },
    "configuration": {
      "title": "coding",
      "properties": {
        "https://api.groq.com/openai/v1/chat/completions": {
          "type": "string",
          "default": "http://localhost:5000/complete",
          "description": "endpoint"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "chatSidebar",
          "title": "AI Chat",
          "icon": "chat.svg"
        }
      ]
    },
    "views": {
      "chatSidebar": [
        {
          "id": "coding.sidebarView",
          "name": "AI Assistant",
          "type": "webview",
          "icon": "chat.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "coding.openChat",
        "title": "Open AI Chat"
      },
      {
        "command": "coding.clearChatHistory",
        "title": "Clear AI Chat History"
      },
      {
        "command": "coding.inFileChat",
        "title": "In file chat",
        "category": "Coding Tools"
      },
      {
        "command": "coding.fixError",
        "title": "Fixing Error",
